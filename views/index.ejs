<html>
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, minimum-scale=1.0, maximum-scale=1.0">

    <title>OpenStreetMap</title>
    <style type="text/css">
    .div-b {
        position: absolute;
        left: 200px;
        top: 20px;
        z-index: 100
    }
    
    .rotate-north {
        left: .65em;
        width: 90px;
        text-align: center;
    }

    .ol-touch .rotate-north {
        color: 'rgba(255, 255, 255, 1)';
        text-align: center;
    }

    .all{
        left: 373px;
        opacity: 0.5;
    }

    .weekdays{
        opacity: 0.5;
        left: 250px;

    }

    .weekends{
        opacity: 0.5;
        left: 450px;
    }

    .axis path,
    .axis line {
      fill: none;
      stroke: #000;
      shape-rendering: crispEdges;
    }

    </style>

    <link rel="stylesheet" type="text/css" href="/bower_components/semantic/dist/semantic.min.css">
    <link rel="stylesheet" type="text/css" href="/stylesheets/lib/ol.css">
    <link rel="stylesheet" type="text/css" href="/stylesheets/lib/jquery.datetimepicker.css">
    <link rel="stylesheet" type="text/css" href="/stylesheets/lib/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="/stylesheets/style.css">
    <link rel="stylesheet" type="text/css" href="/stylesheets/buttons.css">
    <link rel="stylesheet" type="text/css" href="/stylesheets/layout.css">

    <script src="/bower_components/jquery/dist/jquery.min.js" type="text/javascript"></script>
    <script src="/javascripts/lib/bootstrap.min.js" type="text/javascript"></script>
    <script src="/bower_components/semantic/dist/semantic.min.js" type="text/javascript"></script>
    <script src="/javascripts/lib/turf.min.js" type="text/javascript"></script>
