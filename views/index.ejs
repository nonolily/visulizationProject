<div class="main-title">
    TanSeVis
</div>
<div id="content" class="main-content">
    <div class="part-above clearfix">
        <div id="sidebar" CLASS="side-bar">
            <div id="sidebar_content" class="sidebar-content">
                <ul>
                    <li class="car_label_box">
                        <div class="car_list_title">Region View</div>
                        <div class="car_list_box">
                            <div class="input_place clearfloat">
                                <div class="input_title">car label</div>
                                <input placeholder="taxi label" name="KProvince" id="KProvince" />
                                <input class="submit_btn" id = 'submit' type='button' value='Submit' />
                                
                            </div>
                            <div id="search_suggest" class="search-suggest" style=" "></div>
                            <div id="sidebar_loader" style="display: none;">
                                <img alt="Loading..." class="loader" src="/img/image/searching.gif">
                            </div>
                            <div id="car_date" class="car_date" align='center'></div>
                            <div class="show_poi_btn" id = 'kmeans'><a href="#" >K-Means division</a></div>
                            <div class="district_color_bar clearfloat">
                                <div><span class="jinniu-color"></span>Jinniu</div>
                                <div><span class="jinjiang-color"></span>Jinjiang</div>
                                <div><span class="qingyang-color"></span>Qingyang</div>
                                <div><span class="wuhou-color"></span>Wuhou</div>
                                <div><span class="chenghua-color"></span>Chenghua</div>
                            </div>
                        </div>
                    </li>

                    <li>
                        <div class="place_list_title">Traffic Distribution</div>
                        <div class="pieChart"></div>
                    </li>

                    <li>
                        <div class="place_list_title">District Flow</div>
                        <div class="below_chord"></div>
                    </li>
                </ul>

            </div>
        </div>

        <div id="map" tabindex="2" class="div-a leaflet-container leaflet-fade-anim">
        </div>

         <div id="rightBar" class="right_bar">
            <li class="place_box">
                <div class="place_list_title">Road View</div>
                <div class="road_view_title"><div><span>Choose car labels:</span></div>
                </div>
                <div class="choose_C_S_box clearfloat">
                    <div class="choose_C_S active border_right" onclick="getAllLabels(this);">default</div>
                    <div class="choose_C_S border_right" onclick="getAllLabels(this);">count</div>
                    <div class="choose_C_S" onclick="getAllLabels(this);">speed</div>
                </div>
                <div id="carLabelList"></div>
                <div class="road_view_title"><span>Choose places:</span></div>
                <div class="place_list_box">
                    <ul id="roadList">
                        <li>
                            <div flag="0"><span></span>Roads in Qingyang District</div>
                            <ul style="display:none">
                                <li>'Shun Cheng Street'</li>
                            </ul>
                        </li>
                        <li>
                            <div flag="0"><span></span>Roads in Jinniu District</div>
                            <ul style="display:none"></ul>
                        </li>
                        <li>
                            <div flag="0"><span></span>Roads in Wuhou District</div>
                            <ul style="display:none"></ul>
                        </li>
                        <li>
                            <div flag="0"><span></span>Roads in Jinjiang District</div>
                            <ul style="display:none"></ul>
                        </li>
                    </ul>

                </div>
            </li>
            <div>
                <input id="checkPlaces" class="submit_btn" type='button' value='Check the places' onclick="" />
            </div>
        </div>


    </div>
